[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
	hideDotFiles = dotGitOnly
[color]
    ui = auto
	[color "branch"]
		current = cyan
		local = yellow
		remote = green
	[color "diff"]
		meta = yellow bold
		frag = magenta bold
		old = red bold
		new = green bold
	[color "status"]
		added = yellow
		changed = green
		untracked = cyan
[alias]
	st = status
	c = commit
	ca = commit -a
	cm = commit -m
   	cam = commit -a -m
    	ca = commit --amend
    	br = branch
    	co = checkout
    	df = diff
    	lg = log -p
    	lol = log --graph --decorate --pretty=oneline --abbrev-commit
    	lola =  log --graph --decorate --pretty=oneline --abbrev-commit --all
    	lolfp =  log --graph --decorate --pretty=oneline --abbrev-commit --all --first-parent
    	ls = ls-files
	mrg = merge --no-ff
	stat = diff --stat --summary

[remote "origin"]
        url = git@github.com:wdastru/www.ceip.unito.it.git
        fetch = +refs/heads/*:refs/remotes/origin/*
        push = refs/heads/*:refs/heads/*
        push = refs/tags/*:refs/tags/*
        fetch = refs/heads/*:refs/remotes/origin/*
        fetch = refs/tags/*:refs/tags/*

[branch "catalogo"]
	remote = origin
	merge = refs/heads/catalogo
[branch "COST"]
	remote = origin
	merge = refs/heads/COST
[branch "prenotazioni"]
	remote = origin
	merge = refs/heads/prenotazioni
[branch "fantacalcio"]
	remote = origin
	merge = refs/heads/fantacalcio			
[merge]
	conflictstyle = diff3
[branch "INTA"]
	remote = .
	merge = refs/heads/prenotazioni
[branch "Enzo"]
	remote = .
	merge = refs/heads/prenotazioni
[branch "prenotazioni_restyle_600MHz"]
	remote = origin
	merge = refs/heads/prenotazioni_restyle_600MHz
